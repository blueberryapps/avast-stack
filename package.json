    "babel-preset-stage-1": "^6.22.0",
    "css-loader": "^0.27.3",
    "file-loader": "^0.10.1",
    "html-webpack-plugin": "^2.28.0",
    "immutable": "^3.8.1",
    "js-cookie": "^2.1.3",
    "onion-form": "^0.2.0",
    "postcss-loader": "^1.3.3",
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.3",
    "react-router-dom": "^4.0.0",
